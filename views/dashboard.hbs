                <div class="row">
                    <div class="col-md-12 col-lg-12 col-sm-12 col-xs-12">
                        <div class="white-box">
                            <div class="panel panel-default">
                                <div class="panel-heading">Fill up the Coachings details</div>
                                <div class="panel-heading">All Coachings</div>
                                <div class="panel-body">
                                    <div class="col-sm-6">
                                        <div class="col-sm-12 ">
                                            <label for="">Coaching Name*:</label>
                                            <input class="form-control" placeholder="Name" id="name">
                                        </div>
                                        <div class="col-sm-12 spaceTop">
                                            <label for="">Director*:</label>
                                            <input class="form-control" placeholder="Director" id="director">
                                        </div>
                                        <div class="col-sm-12 spaceTop">
                                            <label for="">Mobile*:</label>
                                            <input class="form-control" placeholder="Mobile" id="mobile">
                                        </div>
                                        <div class="col-sm-12 spaceTop">
                                            <label for="">Email:</label>
                                            <input class="form-control" placeholder="Email" id="mail">
                                        </div>
                                        <div class="col-sm-12 spaceTop">
                                            <label for="">Web:</label>
                                            <input class="form-control" placeholder="Website Name" id="web">
                                        </div>
                                        <div class="col-sm-12 spaceTop">
                                            <label for="">Tagline:</label>
                                            <input class="form-control" placeholder="Tagline" id="tag">
                                        </div>
                                        <div class="col-sm-12 spaceTop">
                                            <label for="">Display Section*:</label>
                                            <select class="form-control" id="section">
                                                <option value="select">Select</option>
                                                <option value="UPSC/IAS/PCS">UPSC/IAS/PCS</option>
                                                <option value="TGT/PGT/UGC">TGT/PGT/UGC</option>
                                                <option value="SSC/BANK/RAILWAY/ALL">SSC/BANK/RAILWAY/ALL</option>
                                                <option value="SSCJE/AE">SSCJE/AE</option>
                                                <option value="IIT JEE/NEET">IIT JEE/NEET</option>
                                                <option value="BOARD 10/12">BOARD 10/12</option>
                                                <option value="New Batch">New Batch</option>
                                                <option value="OTHER INDIVIDUAL">OTHER INDIVIDUAL</option>
                                            </select>
                                        </div>
                                        <div class="col-sm-12 spaceTop">
                                            <label for="">Teacher:</label>
                                            <input disabled class="form-control" placeholder="Teacher" id="teacher">
                                        </div>
                                        <div class="col-sm-12 spaceTop">
                                            <label for="">Address*:</label>
                                            <textarea class="form-control" rows="5" placeholder="Address" id="address"></textarea>
                                        </div>

                                        <div class="col-sm-2 spaceTop">
                                            <button class="btn btn-primary" onclick="getValues()">Submit</button>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">

                                        <label class="col-sm-12">Timings:</label>
                                        <div id="education_fields">

                                        </div>
                                        <div class="cols-sm-12">
                                            <div class="col-sm-5 nopadding">
                                                <div class="form-group">
                                                    <input type="text" class="form-control" id="subject1" name="Major[]" value="" placeholder="Subject">
                                                </div>
                                            </div>
                                            <div class="col-sm-2 nopadding">
                                                <div class="form-group">
                                                    <input type="text" class="form-control" id="start1" name="Major[]" value="" placeholder="Start">
                                                </div>
                                            </div>
                                            <div class="col-sm-2 nopadding">
                                                <div class="form-group">
                                                    <input type="text" class="form-control" id="end1" name="Major[]" value="" placeholder="End">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-sm-3 nopadding" style="padding:0px">
                                            <div class="form-group">
                                                <div class="input-group">

                                                    <div class="input-group-btn">
                                                        <button class="btn btn-success" type="button" onclick="education_fields();">
                                                            <span class="glyphicon glyphicon-plus" aria-hidden="true"></span>
                                                        </button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="clear"></div>
                                    </div>

